{
  "name": "tsaphod",
  "version": "0.2.1",
  "description": "Clojure's API for JavaScript's Objects",
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": ">=12.20"
  },
  "scripts": {
    "test": "tape-es index.spec.js | tap-spec",
    "test:types": "tsd",
    "docs": "node tools/make-docs.js",
    "watch-docs": "watch -p 'docs/**' -c 'npm run docs'",
    "cover": "c8 node index.spec.js | tap-spec"
  },
  "keywords": [
    "immutable",
    "clojure",
    "function"
  ],
  "author": "Dan Prince",
  "contributors":[
    {
      "name": "Johan Holmerin"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "c8": "^7.7.2",
    "gray-matter": "^2.0.2",
    "handlebars": "^4.0.5",
    "metalsmith": "^2.2.0",
    "metalsmith-define": "^2.0.1",
    "metalsmith-layouts": "^1.6.5",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-metallic": "^1.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "tape-es": "^1.2.13",
    "tsd": "^0.14.0",
    "typescript": "^4.2.4",
    "watch-cli": "^0.2.1"
  },
  "dependencies": {
    "type-fest": "^1.1.1"
  }
}
